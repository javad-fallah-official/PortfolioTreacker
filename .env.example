# Wallex Python Client Test Configuration

# API Credentials (for testing)
# WALLEX_API_KEY=your_test_api_key_here
# WALLEX_SECRET_KEY=your_test_secret_key_here

# Test Environment
# WALLEX_TESTNET=true

# Rate Limiting
# WALLEX_RATE_LIMIT=1200

# Timeouts (in seconds)
# WALLEX_TIMEOUT=30
# WALLEX_WS_TIMEOUT=60

# Logging
# WALLEX_LOG_LEVEL=INFO

# Test Symbols
# WALLEX_TEST_SYMBOL=BTCUSDT
# WALLEX_TEST_QUOTE_ASSET=USDT

# PostgreSQL Database Configuration
# Option 1: Use direct DSN
# POSTGRES_DSN=postgresql://username:password@localhost:5432/portfolio_tracker

# Option 2: Use individual components (will auto-build DSN)
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_USER=username
# POSTGRES_PASSWORD=password
# POSTGRES_DB=portfolio_tracker